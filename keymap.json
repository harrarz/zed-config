[
  {
    "context": "Workspace",
    "bindings": {
      "shift shift": "file_finder::Toggle",
      "cmd-b": "workspace::ToggleRightDock",
      "cmd-e": "workspace::ToggleLeftDock",
      "cmd-shift-g": "git_panel::ToggleFocus",
      "cmd-shift-i": "agent::ToggleFocus",
      "cmd-q": null,
      "ctrl-cmd-left": "workspace::ActivatePaneLeft",
      "ctrl-cmd-right": "workspace::ActivatePaneRight",
      "ctrl-cmd-up": "workspace::ActivatePaneUp",
      "ctrl-cmd-down": "workspace::ActivatePaneDown",
      "ctrl-cmd-shift-left": "pane::SplitLeft",
      "ctrl-cmd-shift-right": "pane::SplitRight",
      "ctrl-cmd-shift-up": "pane::SplitUp",
      "ctrl-cmd-shift-down": "pane::SplitDown"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-`": "terminal_panel::ToggleFocus",
      "cmd-e": "workspace::ToggleLeftDock",
      "cmd-q": null
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-`": "workspace::ActivatePreviousPane",
      "cmd-e": "workspace::ToggleLeftDock",
      "cmd-q": null
    }
  }
]

